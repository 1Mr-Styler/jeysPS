project 'JPS.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :osx, '10.10'

target 'JPS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for JPS
  pod 'GRDB.swift'
  pod 'SwiftyJSON'

  target 'JPSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'JPSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
